<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.android.onestop.app.DetailActivity.DetailFragment"
    android:id="@+id/event_time">

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/detail_text"
        android:layout_marginRight="126dp"
        android:layout_marginEnd="126dp"
        android:layout_marginTop="18dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_location"
        android:layout_below="@+id/detail_text"
        android:layout_alignLeft="@+id/detail_text"
        android:layout_alignStart="@+id/detail_text"
        android:layout_marginTop="102dp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_reminder"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/text_email"
        android:layout_alignStart="@+id/text_email"
        android:layout_marginBottom="85dp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_email"
        android:layout_above="@+id/text_reminder"
        android:layout_alignLeft="@+id/text_location"
        android:layout_alignStart="@+id/text_location"
        android:layout_marginBottom="105dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@android:drawable/ic_menu_mylocation"
        android:layout_alignBottom="@+id/text_location"
        android:layout_alignLeft="@+id/imageView31"
        android:layout_alignStart="@+id/imageView31"
        android:id="@+id/event_location" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@android:drawable/ic_popup_reminder"
        android:layout_alignTop="@+id/text_email"
        android:layout_alignLeft="@+id/event_location"
        android:layout_alignStart="@+id/event_location"
        android:id="@+id/event_reminder" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@android:drawable/ic_dialog_email"
        android:layout_alignBottom="@+id/text_reminder"
        android:layout_alignRight="@+id/imageView31"
        android:layout_alignEnd="@+id/imageView31"
        android:id="@+id/imageView17" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@android:drawable/ic_menu_recent_history"
        android:layout_alignTop="@+id/detail_text"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/imageView31" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@android:drawable/ic_menu_edit"
        android:layout_below="@+id/imageView17"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="17dp"
        android:layout_marginEnd="17dp"
        android:layout_marginTop="13dp"
        android:id="@+id/imageButton6" />

</RelativeLayout>